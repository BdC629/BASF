        "drupal/better_normalizers": "^1.0",
        "drupal/blazy": "^1.0",
        "drupal/breakpoint_js_settings": "^1.0",
        "drupal/checklistapi": "^1.0",
        "drupal/core": "^8.9",
        "drupal/config_selector": "^1.0",
        "drupal/config_update": "^1.0",
        "drupal/content_lock": "^1.0",
        "drupal/crop": "^1.0",
