        "drupal/metatag": "^1.9",
        "drupal/paragraphs": "^1.11",
        "drupal/paragraphs_features": "^1.8",
        "drupal/password_policy": "3.0-alpha5",
        "drupal/pathauto": "^1.0",
        "drupal/responsive_preview": "^1.0",
        "drupal/redirect": "^1.0",
