{
    "name": "basf/developer_portal_d8",
    "type": "project",
    "description": "Developer portal for BASF with Drupal 8 and Thunder Distribution",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Manuel Heck",
            "email": "manuel-gabriel.heck@partners.basf.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "tubalmartin/yui-css-compressor-php-port",
                "type": "drupal-library",
                "version": "dev-master",
                "dist": {
                    "url": "https://github.com/tubalmartin/YUI-CSS-compressor-PHP-port/archive/master.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "swiftmailer/swiftmailer",
                "type": "drupal-library",
                "version": "5.4.12",
                "dist": {
                    "url": "https://github.com/swiftmailer/swiftmailer/archive/5.x.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "filamentgroup/loadcss",
                "type": "drupal-library",
                "version": "dev-master",
                "dist": {
                    "url": "https://github.com/filamentgroup/loadCSS/archive/master.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "dinbror/blazy",
                "type": "drupal-library",
                "version": "dev-master",
                "dist": {
                    "url": "https://github.com/dinbror/blazy/archive/master.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "moxiecode/plupload",
                "type": "drupal-library",
                "version": "dev-master",
                "dist": {
                    "url": "https://github.com/moxiecode/plupload/archive/master.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "furf/jquery-ui-touch-punch",
                "version": "dev-master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/furf/jquery-ui-touch-punch/archive/master.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "cweagans/composer-patches": "^1.6.5",
