


<div class="{{ classes }}">
  {% if (admin_links is not empty) %}
  <div class="views-admin-links views-hide">
    {{ admin_links }}{% endif %}
  </div>
  {% endif %}
  {% if (header) %}
  <div class="view-header">
    {{ header }}{% endif %}
  </div>
  {% endif %}

  {% if (exposed) %}
  <div class="view-filters">
    {{ exposed }}{% endif %}
  </div>
  {% endif %}

  {% if (attachment_before) %}
  <div class="attachment attachment-before">
    {{ attachment_before }}{% endif %}
  </div>
  {% endif %}

  {% if (pager) %}
  <div class="forum-pager" id="forum-pager-top">{{ pager }}{% endif %}</div>
  {% endif %}

  {% if (rows) %}
  <div class="view-content">
    {{ rows }}{% endif %}
  </div>
  {% elseif (empty) %}
  <div class="view-empty">
    {{ empty }}{% endif %}
  </div>
  {% endif %}

  {% if (pager) %}
  <div class="forum-pager" id="forum-pager-bottom">{{ pager }}{% endif %}</div>
  {% endif %}

  {{ topic_legend }}

  {% if (attachment_after) %}
  <div class="attachment attachment-after">
    {{ attachment_after }}{% endif %}
  </div>
  {% endif %}

  {% if (more) %}
  {{ more }}{% endif %}
  {% endif %}

  {% if (footer) %}
  <div class="view-footer">
    {{ footer }}{% endif %}
  </div>
  {% endif %}

  {% if (feed_icon) %}
  <div class="feed-icon">
    {{ feed_icon }}{% endif %}
  </div>
  {% endif %}

</div> {# class view #}
